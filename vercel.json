{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30,
            "memory": 1024,
            "includeFiles": "admin-assets/**"
        }
    }
}